{
  "name": "@aonawale/react-sanity-hooks",
  "version": "0.0.1",
  "description": "React sanity hooks",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aonawale/react-sanity-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/aonawale/react-sanity-hooks/issues"
  },
  "homepage": "https://aonawale.github.io/react-sanity-hooks/",
  "author": "Ahmed Onawale",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint src --ext .ts --fix",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:unit": "jest -c test/jest.config.js",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "run-s test --watch",
    "cov": "jest -c test/jest.config.js --coverage",
    "doc": "run-s doc:html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "prepare-release": "run-s reset-hard test doc:html version doc:publish",
    "version": "standard-version",
    "prepare": "install-peers"
  },
  "peerDependencies": {
    "@sanity/client": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.6.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  }
}
